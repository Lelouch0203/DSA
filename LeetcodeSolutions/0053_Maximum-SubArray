
from typing import List

def maxSubArray(nums: List[int]) -> int:
    maxi = float('-inf')
    s = 0
    for i in range(len(nums)):
        s += nums[i]
        if s > maxi:
            maxi = s
        if s < 0:
            s = 0
    return int(maxi)

print(maxSubArray([-2, 1, -3, 4, -1, 2, 1, -5, 4]))
